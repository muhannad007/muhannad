      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <!-- Swiper -->
            <div class="swiper-container testimonial-swiper">
              <div class="swiper-wrapper">
                <div class="swiper-slide testimonial-box pink">
                  <!-- <h1 style="font-size: 10rem">+</h1> -->
                  <form class="form" action="/post" method="post">
                    <input
                      type="text"
                      name="title"
                      id="title"
                      placeholder="Title"
                      required
                    />
                    <textarea
                      type="text"
                      name="body"
                      id="testimonial"
                      placeholder="What do you think of Muhannad?"
                      required
                    ></textarea>
                    <input
                      type="text"
                      name="name"
                      id="name"
                      placeholder="Full name"
                      required
                    />
                    <input
                      type="text"
                      name="position"
                      id="position"
                      placeholder="Position/Job title"
                      required
                    />
                    <button class="submit" onclick="approvalAlert()">
                      Add
                    </button>
                  </form>
                </div>
                <% if (testimonials.length > 0) {%> <% colors = [ "yellow",
                "purple", "blue", "pink"]; color_iter = 0;
                testimonials.forEach((testimonial) => {%> <% if
                (testimonial.state == 1) { %>
                <div
                  class="swiper-slide testimonial-box <%= colors[color_iter] %>"
                >
                  <% if (color_iter == 3) color_iter = 0; else color_iter++ %>
                  <h1><%= testimonial.title %></h1>
                  <p><%= testimonial.body %></p>
                  <div class="text-right">
                    <p class="name">- <%= testimonial.name %></p>
                    <p class="designation"><%= testimonial.position %></p>
                  </div>
                </div>
                <% } %> <% }) %> <% } %>
              </div>
              <!-- Add Pagination -->
              <div class="swiper-pagination"></div>
            </div>
          </div>
        </div>
      </div>
    </section>

